package test;

public class Node {
	int value;
	Node next; 
	Node prev;
	
	public Node() {
		
	}
	public   Node(int value) {
		 this.value= value;
		 this.next= null;
		 this.prev=null;
		 
		 System.out.println("inside node constructor");
		 
		 
		 
	} 
	
	@Override
	public String toString() {
		
		
		return " {value=" + value + ", next=" + next + "}";
	}
	 
	
	
	
}
